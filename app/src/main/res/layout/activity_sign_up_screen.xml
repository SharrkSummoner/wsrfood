<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:mask="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#EAEAEA"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#EAEAEA"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/rect3" />

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:foregroundGravity="top"
                        android:scaleType="centerInside"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/imageView6"
                        app:layout_constraintTop_toTopOf="@+id/imageView6"
                        app:srcCompat="@drawable/cooking2" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="23.06dp"
                android:layout_marginRight="23.06dp"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8.02dp"
                    android:alpha="0.4"
                    android:fontFamily="@font/nunito_sans"
                    android:text="E-mail"
                    android:textColor="#000000"
                    android:textSize="15sp" />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginTop="9.01dp"
                    android:fontFamily="@font/nunito_sans"
                    android:hint="deliver@mail.ru"
                    android:inputType="textEmailAddress"
                    android:lines="1"
                    android:outlineAmbientShadowColor="#000000"
                    android:outlineSpotShadowColor="#000000"
                    android:shadowColor="#000000"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:textColorHighlight="#000000"
                    android:textColorHint="#000000"
                    android:textColorLink="#000000"
                    android:textSize="17sp"
                    tools:ignore="TouchTargetSizeCheck" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="37.04dp"
                    android:alpha="0.4"
                    android:fontFamily="@font/nunito_sans"
                    android:text="Password"
                    android:textColor="#000000"
                    android:textSize="15sp" />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginTop="9.01dp"
                    android:fontFamily="@font/nunito_sans"
                    android:hint="* * * * * * * * "
                    android:inputType="textPassword"
                    android:lines="1"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:textColorHighlight="#000000"
                    android:textColorHint="#000000"
                    android:textColorLink="#000000"
                    android:textSize="17sp"
                    tools:ignore="TouchTargetSizeCheck" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="37.04dp"
                    android:alpha="0.4"
                    android:fontFamily="@font/nunito_sans"
                    android:text="Full name"
                    android:textColor="#000000"
                    android:textSize="15sp" />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginTop="9.01dp"
                    android:fontFamily="@font/nunito_sans"
                    android:hint="Olivie Berlusco"
                    android:inputType="textPersonName"
                    android:lines="1"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:textColorHighlight="#000000"
                    android:textColorHint="#000000"
                    android:textColorLink="#000000"
                    android:textSize="17sp"
                    tools:ignore="TouchTargetSizeCheck" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="37.04dp"
                    android:alpha="0.4"
                    android:fontFamily="@font/nunito_sans"
                    android:text="Phone number"
                    android:textColor="#000000"
                    android:textSize="15sp" />

                <EditText
                    android:id="@+id/maskphone"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginTop="9.01dp"
                    android:fontFamily="@font/nunito_sans"
                    android:maxLength="16"
                    android:hint="+7(963)444-12-23"
                    android:inputType="phone"
                    android:lines="1"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:textColorHighlight="#000000"
                    android:textColorHint="#000000"
                    android:textColorLink="#000000"
                    android:textSize="17sp" />


                <Button
                    android:id="@+id/button"
                    android:layout_width="316sp"
                    android:layout_height="70dp"
                    android:layout_marginTop="30.26dp"
                    android:background="@drawable/cornen_radius"
                    android:backgroundTint="#FA4A0C"
                    android:fontFamily="@font/nunito_sans"
                    android:onClick="signup"
                    android:text="Register Now"
                    android:textAllCaps="false"
                    android:textSize="17sp" />

                <Button
                    android:id="@+id/button1"
                    android:layout_width="316sp"
                    android:layout_height="70dp"
                    android:layout_marginTop="17dp"
                    android:background="@drawable/cornen_radius"
                    android:backgroundTint="#FA4A0C"
                    android:fontFamily="@font/nunito_sans"
                    android:onClick="back"
                    android:text="Cancel"
                    android:textAllCaps="false"
                    android:textSize="17sp" />

            </LinearLayout>


        </LinearLayout>
    </ScrollView>
</LinearLayout>